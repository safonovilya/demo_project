<div class="thread"{{if thread.id }} data-id="${thread.id}"{{/if}}>
<b>${thread.msgText}</b>&nbsp-&nbsp<i>${thread.author}</i><span class="re">re</span>
    <ul>
    {{if thread.getChildCount() > 0 }}
        {{each(i, subThread) thread.child}}
            <li>
                {{partial({thread: subThread}) 'thread'}}
            </li>
        {{/each}}
    {{/if}}
    </ul>
</div>